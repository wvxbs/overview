<mvc:View xmlns:mvc="sap.ui.core.mvc" controllerName="com.sap.build.standard.overview.controller.ListaFuncionarios" xmlns="sap.m">
    <Page title="Lista Funcionários" showHeader="false" showFooter="false" showNavButton="false">
        <content>
            <List id="employeeesTable" mode="SingleSelectMaster" width="100%" headerText="Funcionários" footerText="" backgroundDesign="Solid" showSeparators="False" growing="true" growingThreshold="20" growingScrollToLoad="true" visible="true" items="{path: '/'}">
                <infoToolbar>
                    <Toolbar width="100%" height="auto" design="Auto" visible="false" enabled="true">
                        <content>
                            <Label text="Label" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                        </content>
                    </Toolbar>
                </infoToolbar>
                <headerToolbar />
                <items>
                    <ObjectListItem intro="{id}" title="Item 1" number="{employee_salary}" numberUnit="Salário" numberState="None" type="Active" selected="true" showMarkers="false">
                        <attributes>
                            <ObjectAttribute text="{employee_name}" active="false" visible="true"/>
                            <ObjectAttribute text="{employee_age}" active="false" visible="true"/>
                        </attributes>
                    </ObjectListItem>
                </items>
            </List>
        </content>
        <footer>
            <OverflowToolbar width="100%" height="46px" design="Auto" enabled="true" visible="true">
                <content/>
            </OverflowToolbar>
        </footer>
        <headerContent/>
    </Page>
</mvc:View>
